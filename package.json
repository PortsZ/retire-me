{
  "name": "me-aposenta",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "flask-dev": "flask --app src/app/api/python/index run -p 5328 --debugger --reload",
    "flask-build": "flask --app src/app/api/python/index build",
    "flask-start": "flask --app src/app/api/python/index start",
    "next-dev": "NODE_ENV=development next dev",
    "next-build": "NODE_ENV=production next build",
    "next-start": "NODE_ENV=production next start",
    "dev": "concurrently \"npm run next-dev\" \"npm run flask-dev\"",
    "build": "prisma generate && npm run next-build",
    "start": "concurrently \"npm run next-start\" \"npm run flask-start\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@libsql/client": "^0.6.0",
    "@prisma/adapter-libsql": "^5.14.0",
    "@prisma/client": "^5.7.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "framer-motion": "^10.16.16",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.0.2",
    "superstruct": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8",
    "prisma": "^5.7.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
